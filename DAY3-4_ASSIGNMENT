 //QUES 3 solutions 
    //A.Creating array of objects having the specified properties
let Employees=[
    {
        name:"Kilar",
        age:"25",
        country:"panjab",
        Hobbies:["Reading","Writing"],
    },
    {
        name:"sita",
        age:"30",
        country:"India",
        Hobbies:["Dancing","Writing"],
    },
    {
        name:"Kumar",
        age:"40",
        country:"India",
        Hobbies:["Reading","Travelling"],
    }
];
//B.displaying objects in console
for(let i=0 ; i<=2 ; i++)
{
    console.log(Employees[i]);
}

//QUETION 4 SOLUTION
//A.Displaying all the object having age less than 30
function ages(){
    if ((Employees[i].age) < 30){
        console.log(Employees[i].age);
    }

}
//B.display all the object having country india
function cntry(){
    if ((Employees[i].country)=="India"){
        console.log(Employees[i].country);
    }

}

