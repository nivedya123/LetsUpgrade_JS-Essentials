QUESTION ONE SOLUTION 
//HTML CODE
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script src="Domman.js">
    </script>
    <img src="https://images.unsplash.com/photo-1435783459217-ee7fe5414abe?ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60"
    width="500px" id="image"/><br/>
    <button onclick="change()" >FIRST BUTTON </button><br/>
    <button onclick="changeimg()">SECOND BUTTON</button><br/>
    <button onclick="changenextimg()">THIRD BUTTON</button><br/>
</body>
</html>

//Domman.js

//question1-create the webpage contains an image and 3 buttons
//for first button-first image should come back
function change(){
    const ele3=document.getElementById("image");
    const newurl4="https://images.unsplash.com/photo-1435783459217-ee7fe5414abe?ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60";
    ele3.src=newurl4;
}

//for second button - change the existing image to new image
function changeimg(){
    const ele=document.getElementById("image");
    const newUrl="https://images.unsplash.com/photo-1548678756-aa5ed92c4796?ixlib=rb-1.2.1&auto=format&fit=crop&w=500&q=60";
    ele.src=newUrl;
}
//for third button -change it once again
function changenextimg(){
    const ele2=document.getElementById("image");
    const newUrl2="https://images.unsplash.com/photo-1444930694458-01babf71870c?ixlib=rb-1.2.1&auto=format&fit=crop&w=1430&q=80";
    ele2.src=newUrl2;
}

