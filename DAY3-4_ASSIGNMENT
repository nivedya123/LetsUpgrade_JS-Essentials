//HTML CODE
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width", initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script src="ques2.js"></script>
    USER NAME:
    <input type="text"   id="input" placeholder="Enter name" ></br>
    <button onclick="copied()">LOGIN</button> </br>
    PASSWORD:
    <input type="text"  id="output" placeholder="password"></br>
</body>
</html>

//ques2.js

//Ques2:onclick of the button the content of first should be copied into second
//B
function copied(){
    document.getElementById("output").value=document.getElementById("input").value;
    
}
